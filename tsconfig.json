{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "@a/*": ["./src/assets/*"],
      "@c/*": ["./src/components/*"],
      "@h/*": ["./src/hooks/*"],
      "@u/*": ["./src/utils/*"]
    },
    "lib": ["esnext", "scripthost"],
    "allowJs": true,
    "noEmit": true,
    "types": [
      "webpack-env",
      "@dcloudio/types",
      "miniprogram-api-typings",
      "mini-types",
    ]
  },
  "exclude": ["node_modules", "unpackage"]
}
